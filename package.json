    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "4.5.2",
    "typescript-plugin-css-modules": "^3.4.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
